<!DOCTYPE html>
<html lang="en">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		{% if favIconUrl %}
			<link rel="icon" type="image/png" href="{{ constant('ROOT_URL') ~ favIconUrl }}" />
		{% endif %}
		<title>{{ this.title }}</title>
		<script src="bin/jquery-3.1.1.min.js"></script>
		<script src="bin/bundle.js" defer></script>
	</head>

	<body>



				<div id="wrapper">
				  <div id="menu">
				  </div>
				  <div id="header">
				  </div>
					<div id="content">
						{{ toHtml(this.content) }}
					</div>
				</div>

	</body>
</html>
